{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "reset": {
      "dependsOn": ["^reset"],
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    }
  }
}
